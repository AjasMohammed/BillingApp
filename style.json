{
    "QCard": [{
        "cards": [
            "card1",
            "card2",
            "card3",
            "card4",
            "header_frame",
            "recent_title_frame",
            "mid_card1",
            "mid_card2",
            "mid_card3"
        ],
        "shadow":[{
            "color": "#000",
            "blurRadius": 12,
            "xOffset": 0,
            "yOffset": 0
        }]
    }],
    "QCustomSlideMenu": [{
        "name": "menu_side",
        "defaultSize": [{
            "width": 0,
            "height": "parent"
        }],
        "collapsedSize": [{
            "width": 0,
            "height": "parent"
        }],
        "expandedSize": [{
            "width": 230,
            "height": "parent"
        }],
        "toggleButton": [{
            "buttonName": "menu_btn",
            "icons": [{
                "whenMenuIsCollapsed": ":/blueicons/icons/blue_icons/menu.svg",
                "whenMenuIsExpanded": ":/blueicons/icons/blue_icons/chevron-left.svg"
            }]
        }],
        "menuTransitionAnimation": [{
            "animationDuration": 500,
            "animationEasingCurve": "OutBack"
        }]
    }],
    
        "QPushButtonGroup": [{
            "Buttons": [
                {
                    "Name": "dashboard_btn",
                    "IconActive": ":/blueicons/icons/blue_icons/home.svg",
                    "IconInactive": ":/whiteicons/icons/white_icons/home.svg"
                },
                {
                    "Name": "newbill_btn",
                    "IconActive": ":/blueicons/icons/blue_icons/file-plus.svg",
                    "IconInactive": ":/whiteicons/icons/white_icons/file-plus.svg"
                },
                {
                    "Name": "statistics_btn",
                    "IconActive": ":/blueicons/icons/blue_icons/bar-chart.svg",
                    "IconInactive": ":/whiteicons/icons/white_icons/bar-chart.svg"
                },
                {
                    "Name": "logout_btn",
                    "IconActive": ":/blueicons/icons/blue_icons/log-out.svg",
                    "IconInactive": ":/whiteicons/icons/white_icons/log-out.svg"
                }
            ],
            "Style":[{
                "Active": "background-color: #fefeff;border-top-left-radius: 18px ;border-bottom-left-radius: 18px",
                "NotActive": ""
            }]
